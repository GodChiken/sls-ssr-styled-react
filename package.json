{
  "name": "serverless-react-server-side-render",
  "version": "1.0.0",
  "description": "A Demo of Serverless Framework with React Server Side Render",
  "main": "index.js",
  "scripts": {
    "build": "webpack --watch --mode development --config webpack.config.dev.js",
    "serve": "nodemon dist/lambda.js",
    "start": "concurrently \"yarn serve\" \"yarn build\"",
    "s3-sync": "aws s3 sync ./dist/public/ s3://small-wins-static --delete",
    "deploy":
      "webpack --mode production --config webpack.config.prod.js && git add . && git commit -m \"for deploy\" && yarn s3-sync && sls deploy",
    "logs": "sls logs -f lambda"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phodal/serverless-react-server-side-render.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phodal/serverless-react-server-side-render/issues"
  },
  "homepage": "https://github.com/phodal/serverless-react-server-side-render#readme",
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "express": "^4.16.2",
    "helmet": "^3.12.0",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-manifest-plugin": "^2.0.2",
    "webpack-node-externals": "^1.6.0"
  }
}
